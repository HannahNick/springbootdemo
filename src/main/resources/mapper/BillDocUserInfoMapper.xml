<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nick.springboot.mapper.BillDocUserInfoMapper">

    <select id="findByBillDocNo" resultType="com.nick.springboot.entity.BillDocUserInfo">
        select * from bill_doc_user_info where BILL_DOC_NO = #{billDocNo}
    </select>

</mapper>